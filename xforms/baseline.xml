        <item>
          <label ref="jr:itext('/baseline/individualInfo/partialDate/2:label')"/>
          <value>2</value>
        </item>
      </select1>
    </group>
    <input ref="/baseline/dateOfMigration">
      <label ref="jr:itext('/baseline/dateOfMigration:label')"/>
    </input>
    <input ref="/baseline/warning">
      <label ref="jr:itext('/baseline/warning:label')"/>
    </input>
    <input ref="/baseline/visitDate">
      <label ref="jr:itext('/baseline/visitDate:label')"/>
    </input>
    <select1 ref="/baseline/majo4mo">
      <label ref="jr:itext('/baseline/majo4mo:label')"/>
      <item>
        <label ref="jr:itext('/baseline/majo4mo/yes:label')"/>
        <value>yes</value>
      </item>
      <item>
        <label ref="jr:itext('/baseline/majo4mo/no:label')"/>
        <value>no</value>
      </item>
    </select1>
    <select1 ref="/baseline/spelasni">
      <label ref="jr:itext('/baseline/spelasni:label')"/>
      <item>
        <label ref="jr:itext('/baseline/spelasni/yes:label')"/>
        <value>yes</value>
      </item>
      <item>
        <label ref="jr:itext('/baseline/spelasni/no:label')"/>
        <value>no</value>
      </item>
    </select1>
    <input ref="/baseline/processedByMirth">
      <label ref="jr:itext('/baseline/processedByMirth:label')"/>
    </input>
  </h:body>
