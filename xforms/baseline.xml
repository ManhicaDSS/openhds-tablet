      <instance>
        <baseline id="baseline">
          <start/>
          <end/>
          <deviceId/>
          <migrationType/>
          <locationId/>
          <locationName/>
          <visitId/>
          <fieldWorkerId/>
          <individualInfo>
            <individualId/>
            <lastName/>
            <motherId/>
            <fatherId/>
            <firstName/>
            <middleName/>
            <gender/>
            <dateOfBirth/>
            <partialDate/>
          </individualInfo>
          <dateOfMigration/>
          <warning/>
          <visitDate/>
          <majo4mo/>
          <spelasni/>
          <processedByMirth/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </baseline>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/baseline/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/baseline/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/baseline/deviceId" type="string"/>
      <bind nodeset="/baseline/migrationType" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/baseline/locationId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/baseline/locationName" readonly="true()" type="string"/>
      <bind nodeset="/baseline/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/baseline/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/lastName" readonly="true()" relevant=" /baseline/migrationType  = 'BASELINE'" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/motherId" readonly="true()" relevant=" /baseline/migrationType  = 'BASELINE'" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/fatherId" readonly="true()" relevant=" /baseline/migrationType  = 'BASELINE'" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/firstName" relevant=" /baseline/migrationType  = 'BASELINE'" required="true()" type="string"/>
      <bind nodeset="/baseline/individualInfo/middleName" relevant="( /baseline/migrationType  = 'BASELINE') and 1=0" type="string"/>
      <bind nodeset="/baseline/individualInfo/gender" relevant=" /baseline/migrationType  = 'BASELINE'" required="true()" type="select1"/>
      <bind constraint=". &lt;= today()" nodeset="/baseline/individualInfo/dateOfBirth" relevant=" /baseline/migrationType  = 'BASELINE'" required="true()" type="date"/>
      <bind nodeset="/baseline/individualInfo/partialDate" relevant=" /baseline/migrationType  = 'BASELINE'" required="true()" type="select1"/>
      <bind constraint=". &lt;= today()" jr:constraintMsg="the residency date can not be in the future" nodeset="/baseline/dateOfMigration" required="true()" type="date"/>
      <bind nodeset="/baseline/warning" readonly="true()" relevant=" /baseline/individualInfo/dateOfBirth &gt; /baseline/dateOfMigration  and ../migrationType = 'BASELINE'" required="true()" type="string"/>
      <bind constraint=". &lt;= today()" jr:constraintMsg="the visit date can not be in the future" nodeset="/baseline/visitDate" required="true()" type="date"/>
      <bind nodeset="/baseline/majo4mo" required="true()" type="select1"/>
      <bind nodeset="/baseline/spelasni" required="true()" type="select1"/>
      <bind nodeset="/baseline/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/baseline/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;Baseline-&quot;, /baseline/individualInfo/lastName )" nodeset="/baseline/meta/instanceName" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/baseline/migrationType">
      <label ref="jr:itext('/baseline/migrationType:label')"/>
    </input>
    <input ref="/baseline/locationId">
