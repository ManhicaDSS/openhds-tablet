    </input>
    <input ref="/baseline/fieldWorkerId">
      <label ref="jr:itext('/baseline/fieldWorkerId:label')"/>
    </input>
    <group ref="/baseline/individualInfo">
      <label ref="jr:itext('/baseline/individualInfo:label')"/>
      <input ref="/baseline/individualInfo/individualId">
        <label ref="jr:itext('/baseline/individualInfo/individualId:label')"/>
