<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Informação sobre a imunização</h:title>
    <model>
      <instance>
        <imunization id="imunization">
          <deviceid/>
          <start/>
          <end/>
          <visitId/>
          <roundNumber/>
          <fieldWorkerId/>
          <locationId/>
          <individualId/>
          <houseNumber/>
          <region/>
          <permId/>
          <name/>
          <gender/>
          <dob/>
          <fatherPermId/>
          <motherPermId/>
          <vaccination>
            <hasCard/>
            <hasFwViewedCard/>
            <reasonToNotViewCard/>
            <cardStatus/>
            <cardEmissionType/>
            <reasonToNotHaveCard/>
            <isPresent/>
            <hasBcgScar/>
            <hasAllVaccines/>
            <howConfirmed/>
            <vaccinesOnDatabase>2</vaccinesOnDatabase>
            <isPossibleToRegister/>
            <reasonToNotRegister/>
          </vaccination>
          <vaccines>
            <bcg>
              <hasRegisteredBcg>2</hasRegisteredBcg>
              <receivedBcg>2</receivedBcg>
              <receivedBcgDate/>
            </bcg>
            <polioDose0>
              <hasRegisteredPd0>2</hasRegisteredPd0>
              <receivedPd0>2</receivedPd0>
              <receivedPd0Date/>
            </polioDose0>
            <polioDose1>
              <hasRegisteredPd1>2</hasRegisteredPd1>
              <receivedPd1>2</receivedPd1>
              <receivedPd1Date/>
            </polioDose1>
            <polioDose2>
              <hasRegisteredPd2>2</hasRegisteredPd2>
              <receivedPd2>2</receivedPd2>
              <receivedPd2Date/>
            </polioDose2>
            <polioDose3>
              <hasRegisteredPd3>2</hasRegisteredPd3>
              <receivedPd3>2</receivedPd3>
              <receivedPd3Date/>
            </polioDose3>
            <dptDose1>
              <hasRegisteredDpt1>2</hasRegisteredDpt1>
              <receivedDpt1>2</receivedDpt1>
              <receivedDpt1Date/>
            </dptDose1>
            <dptDose2>
              <hasRegisteredDpt2>2</hasRegisteredDpt2>
              <receivedDpt2>2</receivedDpt2>
              <receivedDpt2Date/>
            </dptDose2>
            <dptDose3>
              <hasRegisteredDpt3>2</hasRegisteredDpt3>
              <receivedDpt3>2</receivedDpt3>
              <receivedDpt3Date/>
            </dptDose3>
            <pcv10Dose1>
              <hasRegisteredPcv1>2</hasRegisteredPcv1>
              <receivedPcv1>2</receivedPcv1>
              <receivedPcv1Date/>
            </pcv10Dose1>
            <pcv10Dose2>
              <hasRegisteredPcv2>2</hasRegisteredPcv2>
              <receivedPcv2>2</receivedPcv2>
              <receivedPcv2Date/>
            </pcv10Dose2>
            <pcv10Dose3>
              <hasRegisteredPcv3>2</hasRegisteredPcv3>
              <receivedPcv3>2</receivedPcv3>
              <receivedPcv3Date/>
            </pcv10Dose3>
            <sarampo>
              <hasRegisteredSmp>2</hasRegisteredSmp>
              <receivedSmp>2</receivedSmp>
              <receivedSmpDate/>
            </sarampo>
            <rotavirusDose1>
              <hasRegisteredRv1>2</hasRegisteredRv1>
              <receivedRv1>2</receivedRv1>
              <receivedRv1Date/>
            </rotavirusDose1>
            <rotavirusDose2>
              <hasRegisteredRv2>2</hasRegisteredRv2>
              <receivedRv2>2</receivedRv2>
              <receivedRv2Date/>
            </rotavirusDose2>
            <rotavirusDose3>
              <hasRegisteredRv3>2</hasRegisteredRv3>
              <receivedRv3>2</receivedRv3>
              <receivedRv3Date/>
            </rotavirusDose3>
            <vitaminaATotal>0</vitaminaATotal>
            <vitaminaANrDoses>0</vitaminaANrDoses>
            <vitaminaA_count/>
            <vitaminaA jr:template="">
              <vaNumber/>
              <receivedVtaDate/>
            </vitaminaA>
            <mebendazolTotal>0</mebendazolTotal>
            <mebendazolNrDoses>0</mebendazolNrDoses>
            <mebendazol_count/>
            <mebendazol jr:template="">
              <mbNumber/>
              <receivedMbzDate/>
            </mebendazol>
            <othersTotal>0</othersTotal>
            <othersNrVaccines>0</othersNrVaccines>
            <others_count/>
            <others jr:template="">
              <otNumber/>
              <otName/>
              <receivedDate/>
            </others>
          </vaccines>
          <processedBySync/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </imunization>
      </instance>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/imunization/deviceid" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/imunization/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/imunization/end" type="dateTime"/>
      <bind nodeset="/imunization/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/imunization/roundNumber" readonly="true()" required="true()" type="int"/>
      <bind nodeset="/imunization/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/imunization/locationId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/imunization/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/imunization/houseNumber" readonly="true()" required="true()" type="string"/>
      <bind calculate="substr( /imunization/houseNumber , 0, 4)" nodeset="/imunization/region" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/imunization/permId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/imunization/name" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/imunization/gender" readonly="true()" required="true()" type="select1"/>
      <bind constraint="(int((today() - .) div 365.24) &lt;= 5) and (today() &gt;= .)" jr:constraintMsg="Este formulario é para crianças menores de 5 anos (NB: a data de nascimento não pode ser maior que a data de hoje)" nodeset="/imunization/dob" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/imunization/fatherPermId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/imunization/motherPermId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/imunization/vaccination" relevant="(int((today() -  /imunization/dob ) div 365.24) &lt;= 5) and (today() &gt;=  /imunization/dob )"/>
      <bind nodeset="/imunization/vaccination/hasCard" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/hasFwViewedCard" relevant=" /imunization/vaccination/hasCard =1" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/reasonToNotViewCard" relevant=" /imunization/vaccination/hasFwViewedCard =2" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/cardStatus" relevant=" /imunization/vaccination/hasFwViewedCard =1" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/cardEmissionType" relevant=" /imunization/vaccination/hasFwViewedCard =1" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/reasonToNotHaveCard" relevant=" /imunization/vaccination/hasCard =2" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/isPresent" relevant=" /imunization/vaccination/hasCard &gt;=2" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/hasBcgScar" relevant=" /imunization/vaccination/isPresent =1" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/hasAllVaccines" relevant=" /imunization/vaccination/hasCard =1" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/howConfirmed" relevant=" /imunization/vaccination/hasAllVaccines =1" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/vaccinesOnDatabase" readonly="true()" relevant=" /imunization/vaccination/hasCard =1" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/isPossibleToRegister" relevant=" /imunization/vaccination/vaccinesOnDatabase =2" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccination/reasonToNotRegister" relevant=" /imunization/vaccination/isPossibleToRegister =2" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines" relevant=" /imunization/vaccination/isPossibleToRegister =1"/>
      <bind nodeset="/imunization/vaccines/bcg/hasRegisteredBcg" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/bcg/receivedBcg" readonly=" /imunization/vaccines/bcg/hasRegisteredBcg =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/bcg/receivedBcgDate" readonly=" /imunization/vaccines/bcg/hasRegisteredBcg =1" relevant=" /imunization/vaccines/bcg/receivedBcg =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/polioDose0/hasRegisteredPd0" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/polioDose0/receivedPd0" readonly=" /imunization/vaccines/polioDose0/hasRegisteredPd0 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/polioDose0/receivedPd0Date" readonly=" /imunization/vaccines/polioDose0/hasRegisteredPd0 =1" relevant=" /imunization/vaccines/polioDose0/receivedPd0 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/polioDose1/hasRegisteredPd1" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/polioDose1/receivedPd1" readonly=" /imunization/vaccines/polioDose1/hasRegisteredPd1 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/polioDose1/receivedPd1Date" readonly=" /imunization/vaccines/polioDose1/hasRegisteredPd1 =1" relevant=" /imunization/vaccines/polioDose1/receivedPd1 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/polioDose2/hasRegisteredPd2" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/polioDose2/receivedPd2" readonly=" /imunization/vaccines/polioDose2/hasRegisteredPd2 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/polioDose2/receivedPd2Date" readonly=" /imunization/vaccines/polioDose2/hasRegisteredPd2 =1" relevant=" /imunization/vaccines/polioDose2/receivedPd2 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/polioDose3/hasRegisteredPd3" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/polioDose3/receivedPd3" readonly=" /imunization/vaccines/polioDose3/hasRegisteredPd3 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/polioDose3/receivedPd3Date" readonly=" /imunization/vaccines/polioDose3/hasRegisteredPd3 =1" relevant=" /imunization/vaccines/polioDose3/receivedPd3 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/dptDose1/hasRegisteredDpt1" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/dptDose1/receivedDpt1" readonly=" /imunization/vaccines/dptDose1/hasRegisteredDpt1 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/dptDose1/receivedDpt1Date" readonly=" /imunization/vaccines/dptDose1/hasRegisteredDpt1 =1" relevant=" /imunization/vaccines/dptDose1/receivedDpt1 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/dptDose2/hasRegisteredDpt2" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/dptDose2/receivedDpt2" readonly=" /imunization/vaccines/dptDose2/hasRegisteredDpt2 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/dptDose2/receivedDpt2Date" readonly=" /imunization/vaccines/dptDose2/hasRegisteredDpt2 =1" relevant=" /imunization/vaccines/dptDose2/receivedDpt2 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/dptDose3/hasRegisteredDpt3" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/dptDose3/receivedDpt3" readonly=" /imunization/vaccines/dptDose3/hasRegisteredDpt3 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/dptDose3/receivedDpt3Date" readonly=" /imunization/vaccines/dptDose3/hasRegisteredDpt3 =1" relevant=" /imunization/vaccines/dptDose3/receivedDpt3 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/pcv10Dose1/hasRegisteredPcv1" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/pcv10Dose1/receivedPcv1" readonly=" /imunization/vaccines/pcv10Dose1/hasRegisteredPcv1 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/pcv10Dose1/receivedPcv1Date" readonly=" /imunization/vaccines/pcv10Dose1/hasRegisteredPcv1 =1" relevant=" /imunization/vaccines/pcv10Dose1/receivedPcv1 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/pcv10Dose2/hasRegisteredPcv2" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/pcv10Dose2/receivedPcv2" readonly=" /imunization/vaccines/pcv10Dose2/hasRegisteredPcv2 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/pcv10Dose2/receivedPcv2Date" readonly=" /imunization/vaccines/pcv10Dose2/hasRegisteredPcv2 =1" relevant=" /imunization/vaccines/pcv10Dose2/receivedPcv2 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/pcv10Dose3/hasRegisteredPcv3" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/pcv10Dose3/receivedPcv3" readonly=" /imunization/vaccines/pcv10Dose3/hasRegisteredPcv3 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/pcv10Dose3/receivedPcv3Date" readonly=" /imunization/vaccines/pcv10Dose3/hasRegisteredPcv3 =1" relevant=" /imunization/vaccines/pcv10Dose3/receivedPcv3 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/sarampo/hasRegisteredSmp" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/sarampo/receivedSmp" readonly=" /imunization/vaccines/sarampo/hasRegisteredSmp =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/sarampo/receivedSmpDate" readonly=" /imunization/vaccines/sarampo/hasRegisteredSmp =1" relevant=" /imunization/vaccines/sarampo/receivedSmp =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/rotavirusDose1/hasRegisteredRv1" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/rotavirusDose1/receivedRv1" readonly=" /imunization/vaccines/rotavirusDose1/hasRegisteredRv1 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/rotavirusDose1/receivedRv1Date" readonly=" /imunization/vaccines/rotavirusDose1/hasRegisteredRv1 =1" relevant=" /imunization/vaccines/rotavirusDose1/receivedRv1 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/rotavirusDose2/hasRegisteredRv2" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/rotavirusDose2/receivedRv2" readonly=" /imunization/vaccines/rotavirusDose2/hasRegisteredRv2 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/rotavirusDose2/receivedRv2Date" readonly=" /imunization/vaccines/rotavirusDose2/hasRegisteredRv2 =1" relevant=" /imunization/vaccines/rotavirusDose2/receivedRv2 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/rotavirusDose3/hasRegisteredRv3" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/imunization/vaccines/rotavirusDose3/receivedRv3" readonly=" /imunization/vaccines/rotavirusDose3/hasRegisteredRv3 =1" required="true()" type="select1"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/rotavirusDose3/receivedRv3Date" readonly=" /imunization/vaccines/rotavirusDose3/hasRegisteredRv3 =1" relevant=" /imunization/vaccines/rotavirusDose3/receivedRv3 =1" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/vitaminaATotal" readonly="true()" required="true()" type="int"/>
      <bind constraint="(. &gt;=0) and ( /imunization/vaccines/vitaminaATotal +.)&lt;=10" jr:constraintMsg="O número de doses recebidas + as doses que deseja adicionar não pode ser maior que 10 ou menor que zero" nodeset="/imunization/vaccines/vitaminaANrDoses" required="true()" type="int"/>
      <bind calculate=" /imunization/vaccines/vitaminaANrDoses " nodeset="/imunization/vaccines/vitaminaA_count" readonly="true()" type="string"/>
      <bind nodeset="/imunization/vaccines/vitaminaA" relevant=" /imunization/vaccines/vitaminaANrDoses &gt;0"/>
      <bind calculate=" /imunization/vaccines/vitaminaATotal +position(..)" nodeset="/imunization/vaccines/vitaminaA/vaNumber" type="string"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/vitaminaA/receivedVtaDate" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/mebendazolTotal" readonly="true()" required="true()" type="int"/>
      <bind constraint="(. &gt;=0) and ( /imunization/vaccines/mebendazolTotal +.)&lt;=10" jr:constraintMsg="O número de doses recebidas + as doses que deseja adicionar não pode ser maior que 10 ou menor que zero" nodeset="/imunization/vaccines/mebendazolNrDoses" required="true()" type="int"/>
      <bind calculate=" /imunization/vaccines/mebendazolNrDoses " nodeset="/imunization/vaccines/mebendazol_count" readonly="true()" type="string"/>
      <bind nodeset="/imunization/vaccines/mebendazol" relevant=" /imunization/vaccines/mebendazolNrDoses &gt;0"/>
      <bind calculate=" /imunization/vaccines/mebendazolTotal +position(..)" nodeset="/imunization/vaccines/mebendazol/mbNumber" type="string"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/mebendazol/receivedMbzDate" required="true()" type="date"/>
      <bind nodeset="/imunization/vaccines/othersTotal" readonly="true()" required="true()" type="int"/>
      <bind constraint="(. &gt;=0) and ( /imunization/vaccines/othersTotal +.)&lt;=10" jr:constraintMsg="O número de doses recebidas + as doses que deseja adicionar não pode ser maior que 10 ou menor que zero" nodeset="/imunization/vaccines/othersNrVaccines" required="true()" type="int"/>
      <bind calculate=" /imunization/vaccines/othersNrVaccines " nodeset="/imunization/vaccines/others_count" readonly="true()" type="string"/>
      <bind nodeset="/imunization/vaccines/others" relevant=" /imunization/vaccines/othersNrVaccines &gt;0"/>
      <bind calculate=" /imunization/vaccines/othersTotal +position(..)" nodeset="/imunization/vaccines/others/otNumber" type="string"/>
      <bind nodeset="/imunization/vaccines/others/otName" required="true()" type="string"/>
      <bind constraint="(. &gt;=  /imunization/dob ) and (. &lt;= today())" jr:constraintMsg="Esta data não pode ser menor que a data de nasc. ou maior que a data de hoje" nodeset="/imunization/vaccines/others/receivedDate" required="true()" type="date"/>
      <bind nodeset="/imunization/processedBySync" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/imunization/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;Imunization_Dtls_&quot;, /imunization/permId )" nodeset="/imunization/meta/instanceName" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/imunization/visitId">
      <label>1. Id da Visita</label>
    </input>
    <input ref="/imunization/roundNumber">
      <label>2. Número da ronda</label>
    </input>
    <input ref="/imunization/fieldWorkerId">
      <label>3. Código do Inquiridor</label>
    </input>
    <input ref="/imunization/locationId">
      <label>4. ExtId do Agregado</label>
    </input>
    <input ref="/imunization/individualId">
      <label>5. ExtId do Individuo</label>
    </input>
    <input ref="/imunization/houseNumber">
      <label>6. Número do Agregado</label>
    </input>
    <input ref="/imunization/region">
      <label>7. Bairro</label>
    </input>
    <input ref="/imunization/permId">
      <label>8. Perm ID</label>
    </input>
    <input ref="/imunization/name">
      <label>9. Nome</label>
    </input>
    <select1 ref="/imunization/gender">
      <label>10. Sexo</label>
      <item>
        <label>Masculino</label>
        <value>M</value>
      </item>
      <item>
        <label>Feminino</label>
        <value>F</value>
      </item>
