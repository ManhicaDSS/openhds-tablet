          <start/>
          <end/>
          <deviceid/>
          <visitId/>
          <visitDate/>
          <fieldWorkerId/>
          <householdId/>
          <householdName/>
          <new_hoh_id/>
          <new_hoh_name/>
          <eventDate/>
          <causes/>
          <otherCauses/>
          <hh_people_nb/>
          <membershiptonewhoh_count/>
          <membershiptonewhoh jr:template="">
            <extId/>
            <memberName/>
            <socialGroupId/>
            <relationshipToGroupHead/>
          </membershiptonewhoh>
          <processedByMirth/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </change_household_head>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/change_household_head/start" readonly="true()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/change_household_head/end" readonly="true()" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/change_household_head/deviceid" readonly="true()" type="string"/>
      <bind nodeset="/change_household_head/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_household_head/visitDate" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/change_household_head/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_household_head/householdId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_household_head/householdName" readonly="true()" type="string"/>
      <bind nodeset="/change_household_head/new_hoh_id" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_household_head/new_hoh_name" readonly="true()" type="string"/>
      <bind nodeset="/change_household_head/eventDate" required="true()" type="date"/>
      <bind nodeset="/change_household_head/causes" required="true()" type="select1"/>
      <bind nodeset="/change_household_head/otherCauses" relevant="selected( /change_household_head/causes , '99')" required="true()" type="string"/>
      <bind nodeset="/change_household_head/hh_people_nb" readonly="true()" relevant="false()" required="true()" type="int"/>
      <bind calculate=" /change_household_head/hh_people_nb " nodeset="/change_household_head/membershiptonewhoh_count" readonly="true()" type="string"/>
      <bind nodeset="/change_household_head/membershiptonewhoh/extId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_household_head/membershiptonewhoh/memberName" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_household_head/membershiptonewhoh/socialGroupId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/change_household_head/membershiptonewhoh/relationshipToGroupHead" required="true()" type="select1"/>
      <bind nodeset="/change_household_head/processedByMirth" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/change_household_head/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;CHANGE_HOH-&quot;, /change_household_head/householdName )" nodeset="/change_household_head/meta/instanceName" type="string"/>
    </model>
