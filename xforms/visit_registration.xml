      <instance>
        <visit_registration id="visit_registration">
          <start/>
          <end/>
          <deviceid/>
          <visitId/>
          <fieldWorkerId/>
          <locationId/>
          <locationName/>
          <roundNumber/>
          <locationIdCheck/>
          <locationcalculated/>
          <checkRightId/>
          <whynotrightcode/>
          <visitDate/>
          <intervieweeId/>
          <completedQuest/>
          <reasonToNotComplete/>
          <reason/>
          <correctInterviewee/>
          <intervieweeName/>
          <farmhouse/>
          <houseNoLocated/>
          <houseNoNeedRepaint/>
          <neighbors>
            <house1/>
            <house2/>
            <house3/>
          </neighbors>
          <coordinates/>
          <processedByMirth/>
          <processedBySync/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </visit_registration>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/visit_registration/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/visit_registration/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/visit_registration/deviceid" type="string"/>
      <bind nodeset="/visit_registration/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/visit_registration/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/visit_registration/locationId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/visit_registration/locationName" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/visit_registration/roundNumber" readonly="true()" required="true()" type="int"/>
      <bind nodeset="/visit_registration/locationIdCheck" relevant="1=0" type="barcode"/>
      <bind calculate="concat(substr( /visit_registration/locationIdCheck ,0,3),'0000',substr( /visit_registration/locationIdCheck ,3,5))" nodeset="/visit_registration/locationcalculated" readonly="true()" relevant="1=0" type="string"/>
      <bind nodeset="/visit_registration/checkRightId" relevant="1=0 and ( /visit_registration/locationId  != /visit_registration/locationcalculated  and  /visit_registration/locationId  != /visit_registration/locationIdCheck )" type="string"/>
      <bind nodeset="/visit_registration/whynotrightcode" relevant="1=0 and ( /visit_registration/locationId  != /visit_registration/locationcalculated  and  /visit_registration/locationId  != /visit_registration/locationIdCheck )" type="select1"/>
      <bind nodeset="/visit_registration/visitDate" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/visit_registration/intervieweeId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/visit_registration/completedQuest" required="true()" type="select1"/>
      <bind nodeset="/visit_registration/reasonToNotComplete" relevant=" /visit_registration/completedQuest =2" required="true()" type="select1"/>
      <bind nodeset="/visit_registration/reason" relevant=" /visit_registration/completedQuest =1" required="true()" type="select1"/>
