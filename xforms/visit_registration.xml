      <bind nodeset="/visit_registration/neighbors" relevant=" /visit_registration/completedQuest =1 and ( /visit_registration/reason =1 or  /visit_registration/reason =5 or  /visit_registration/reason =6)"/>
      <bind constraint="(regex(., '^[0-9]{4}-[0-9]{3}$'))" jr:constraintMsg="O formato do numero de agregado deve ser XXXX-XXX" nodeset="/visit_registration/neighbors/house1" required="true()" type="string"/>
