      <bind nodeset="/visit_registration/coordinates" relevant=" /visit_registration/farmhouse  = '1' and  /visit_registration/completedQuest =1 and ( /visit_registration/reason =1 or  /visit_registration/reason =5 or  /visit_registration/reason =6)" type="geopoint"/>
      <bind nodeset="/visit_registration/processedByMirth" readonly="true()" type="int"/>
      <bind nodeset="/visit_registration/processedBySync" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/visit_registration/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;Visita_&quot;, /visit_registration/visitId ,&quot;-&quot;, format-date( /visit_registration/visitDate ,&quot;%Y%n%e&quot;))" nodeset="/visit_registration/meta/instanceName" type="string"/>
