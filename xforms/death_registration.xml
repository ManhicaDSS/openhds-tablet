      <instance>
        <death_registration id="death_registration">
          <start/>
          <end/>
          <deviceid/>
          <visitId/>
          <fieldWorkerId/>
          <individualId/>
          <permId/>
          <name/>
          <dateOfDeath/>
          <causeOfDeath/>
          <disease/>
          <placeOfDeath/>
          <placeOfDeathOther/>
          <processedByMirth/>
          <processedBySync/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </death_registration>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/death_registration/start" readonly="true()" required="true()" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/death_registration/end" readonly="true()" required="true()" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/death_registration/deviceid" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_registration/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_registration/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_registration/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_registration/permId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/death_registration/name" readonly="true()" required="true()" type="string"/>
      <bind constraint=". &lt;= today()" nodeset="/death_registration/dateOfDeath" required="true()" type="date"/>
      <bind nodeset="/death_registration/causeOfDeath" required="true()" type="select1"/>
      <bind nodeset="/death_registration/disease" relevant=" /death_registration/causeOfDeath =1" required="true()" type="select1"/>
      <bind nodeset="/death_registration/placeOfDeath" required="true()" type="select1"/>
      <bind nodeset="/death_registration/placeOfDeathOther" relevant="selected( /death_registration/placeOfDeath , '5')" required="true()" type="string"/>
      <bind nodeset="/death_registration/processedByMirth" readonly="true()" type="int"/>
      <bind nodeset="/death_registration/processedBySync" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/death_registration/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;Obito_&quot;,  /death_registration/permId )" nodeset="/death_registration/meta/instanceName" type="string"/>
    </model>
