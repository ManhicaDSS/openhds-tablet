      <instance>
        <pregnancy_observation id="pregnancy_observation">
          <start/>
          <end/>
          <deviceid/>
          <visitId/>
          <fieldWorkerId/>
          <individualId/>
          <permId/>
          <name/>
          <recordedDate/>
          <estimatedAgeOfPreg/>
          <numberOfPregncancies/>
          <lostPregnancies/>
          <expectedDeliveryDate/>
          <medicineforpregnancy/>
          <ttinjection/>
          <ttinjectionDates/>
          <processedByMirth/>
          <processedBySync/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </pregnancy_observation>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/pregnancy_observation/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/pregnancy_observation/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/pregnancy_observation/deviceid" type="string"/>
      <bind nodeset="/pregnancy_observation/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_observation/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_observation/individualId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_observation/permId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_observation/name" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/pregnancy_observation/recordedDate" readonly="true()" required="true()" type="date"/>
      <bind nodeset="/pregnancy_observation/estimatedAgeOfPreg" required="true()" type="int"/>
      <bind nodeset="/pregnancy_observation/numberOfPregncancies" required="true()" type="int"/>
      <bind constraint=". &gt;= 0" jr:constraintMsg="O numero não pode ser negativo" nodeset="/pregnancy_observation/lostPregnancies" required="true()" type="int"/>
      <bind constraint=".&gt;= today()" nodeset="/pregnancy_observation/expectedDeliveryDate" required="true()" type="date"/>
      <bind nodeset="/pregnancy_observation/medicineforpregnancy" required="true()" type="select1"/>
      <bind nodeset="/pregnancy_observation/ttinjection" required="true()" type="select1"/>
      <bind nodeset="/pregnancy_observation/ttinjectionDates" relevant=" /pregnancy_observation/ttinjection =1" required="true()" type="string"/>
      <bind nodeset="/pregnancy_observation/processedByMirth" readonly="true()" type="int"/>
      <bind nodeset="/pregnancy_observation/processedBySync" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/pregnancy_observation/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;ObsGravidez_&quot;,  /pregnancy_observation/permId , &quot;_&quot;, format-date(today(),&quot;%Y%n%e&quot;))" nodeset="/pregnancy_observation/meta/instanceName" type="string"/>
    </model>
