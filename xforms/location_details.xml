<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Informação sobre a Casa</h:title>
    <model>
      <instance>
        <location_details id="location_details">
          <deviceid/>
          <start/>
          <end/>
          <visitId/>
          <roundNumber/>
          <fieldWorkerId/>
          <locationId/>
          <houseNumber/>
          <region/>
          <headName/>
          <headPermId/>
          <hasSubsHead/>
          <subsHeadName/>
          <subsHeadPermId/>
          <sec1>
            <houseNoLocated/>
            <houseNoNeedRepaint/>
            <nrConstructions/>
            <nrBedrooms/>
            <nrHouseDivisions/>
            <nrHouseDivUsed/>
            <habitationType/>
            <habitationName/>
            <wallMaterial/>
            <wallMaterialName/>
            <coverageMaterial/>
            <coverageMaterialName/>
            <floorMaterial/>
            <floorMaterialName/>
            <cellNumbers/>
          </sec1>
          <sec2>
            <hasKitchen/>
            <isKitchenInside/>
            <kitchenHasWall/>
            <kitchenHasCoverage/>
            <kitchenFuel/>
            <kitchenFuelName/>
            <iluminationFuel/>
            <iluminationFuelName/>
          </sec2>
          <hasSec3/>
          <sec3>
            <isWaterSrcInside/>
            <waterSource/>
            <waterSourceName/>
            <outsideWaterSrc/>
            <outsideWaterSrcName/>
            <timeToCollectWater/>
            <watTreatmentProc/>
            <watTreatmentProcName/>
            <hasWaterRecipient/>
            <canSeeRecipient/>
            <waterRecptElevated/>
            <waterRecptCovered/>
            <howWaterCollected/>
            <howWaterCollectedName/>
            <wuWellMaintained/>
            <waterSupplier/>
            <waterMoney/>
          </sec3>
          <hasSec4/>
          <sec4>
            <hasLatrine/>
            <latrineType/>
            <pooTreatment/>
            <canSeeLatrine/>
            <cleanedLatrine/>
            <latrineHasPrivacy/>
            <washAfterPoop/>
            <otherPoopPlace/>
          </sec4>
          <hasSec5/>
          <sec5>
            <trashDeposit/>
            <trashDepositName/>
            <trashTreatment/>
            <trashTreatmentName/>
            <trashRecycledPart/>
            <trashRecycleMethod/>
            <trashUsedAsFertilizer/>
          </sec5>
          <ine>
            <registered/>
            <reasonToNotReg/>
            <otherReasonToNotReg/>
            <tagAtDoor/>
          </ine>
          <processedByMirth/>
          <processedBySync/>
          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </location_details>
      </instance>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/location_details/deviceid" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/location_details/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/location_details/end" type="dateTime"/>
      <bind nodeset="/location_details/visitId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/location_details/roundNumber" readonly="true()" required="true()" type="int"/>
      <bind nodeset="/location_details/fieldWorkerId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/location_details/locationId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/location_details/houseNumber" readonly="true()" required="true()" type="string"/>
      <bind calculate="substr( /location_details/houseNumber , 0, 4)" nodeset="/location_details/region" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/location_details/headName" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/location_details/headPermId" readonly="true()" required="true()" type="string"/>
      <bind nodeset="/location_details/hasSubsHead" readonly="true()" required="true()" type="select1"/>
      <bind nodeset="/location_details/subsHeadName" readonly="true()" relevant=" /location_details/hasSubsHead =1" required="true()" type="string"/>
      <bind nodeset="/location_details/subsHeadPermId" readonly="true()" relevant=" /location_details/hasSubsHead =1" required="true()" type="string"/>
      <bind nodeset="/location_details/sec1/houseNoLocated" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec1/houseNoNeedRepaint" required="true()" type="select1"/>
      <bind constraint=". &gt; 0" jr:constraintMsg="O valor deve ser maior que zero" nodeset="/location_details/sec1/nrConstructions" required="true()" type="int"/>
      <bind constraint=". &gt; 0" nodeset="/location_details/sec1/nrBedrooms" required="true()" type="int"/>
      <bind constraint=". &gt; 0" jr:constraintMsg="O valor deve ser maior que zero" nodeset="/location_details/sec1/nrHouseDivisions" required="true()" type="int"/>
      <bind nodeset="/location_details/sec1/nrHouseDivUsed" required="true()" type="int"/>
      <bind nodeset="/location_details/sec1/habitationType" required="true()" type="select"/>
      <bind nodeset="/location_details/sec1/habitationName" relevant="selected( /location_details/sec1/habitationType , '98')" required="true()" type="string"/>
      <bind nodeset="/location_details/sec1/wallMaterial" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec1/wallMaterialName" relevant=" /location_details/sec1/wallMaterial =98" required="true()" type="string"/>
      <bind nodeset="/location_details/sec1/coverageMaterial" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec1/coverageMaterialName" relevant=" /location_details/sec1/coverageMaterial =98" required="true()" type="string"/>
      <bind nodeset="/location_details/sec1/floorMaterial" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec1/floorMaterialName" relevant=" /location_details/sec1/floorMaterial =98" required="true()" type="string"/>
      <bind nodeset="/location_details/sec1/cellNumbers" required="true()" type="string"/>
      <bind nodeset="/location_details/sec2/hasKitchen" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec2/isKitchenInside" relevant=" /location_details/sec2/hasKitchen =1" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec2/kitchenHasWall" relevant=" /location_details/sec2/hasKitchen =1 and  /location_details/sec2/isKitchenInside =2" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec2/kitchenHasCoverage" relevant=" /location_details/sec2/hasKitchen =1 and  /location_details/sec2/isKitchenInside =2" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec2/kitchenFuel" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec2/kitchenFuelName" relevant=" /location_details/sec2/kitchenFuel =98" required="true()" type="string"/>
      <bind nodeset="/location_details/sec2/iluminationFuel" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec2/iluminationFuelName" relevant=" /location_details/sec2/iluminationFuel =98" required="true()" type="string"/>
      <bind nodeset="/location_details/hasSec3" type="select1"/>
      <bind nodeset="/location_details/sec3" relevant=" /location_details/hasSec3 =1"/>
      <bind nodeset="/location_details/sec3/isWaterSrcInside" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec3/waterSource" relevant=" /location_details/sec3/isWaterSrcInside =1" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec3/waterSourceName" relevant=" /location_details/sec3/isWaterSrcInside =1 and  /location_details/sec3/waterSource =98" required="true()" type="string"/>
      <bind nodeset="/location_details/sec3/outsideWaterSrc" relevant=" /location_details/sec3/isWaterSrcInside =2" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec3/outsideWaterSrcName" relevant=" /location_details/sec3/outsideWaterSrc =98 and  /location_details/sec3/isWaterSrcInside =2" required="true()" type="string"/>
      <bind nodeset="/location_details/sec3/timeToCollectWater" relevant=" /location_details/sec3/isWaterSrcInside =2" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec3/watTreatmentProc" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec3/watTreatmentProcName" relevant=" /location_details/sec3/watTreatmentProc =5" required="true()" type="string"/>
      <bind nodeset="/location_details/sec3/hasWaterRecipient" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec3/canSeeRecipient" relevant=" /location_details/sec3/hasWaterRecipient =1" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec3/waterRecptElevated" relevant=" /location_details/sec3/canSeeRecipient =1 or  /location_details/sec3/canSeeRecipient =2" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec3/waterRecptCovered" relevant=" /location_details/sec3/canSeeRecipient =1 or  /location_details/sec3/canSeeRecipient =2" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec3/howWaterCollected" relevant=" /location_details/sec3/canSeeRecipient =1 or  /location_details/sec3/canSeeRecipient =2" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec3/howWaterCollectedName" relevant=" /location_details/sec3/howWaterCollected =98" required="true()" type="string"/>
      <bind nodeset="/location_details/sec3/wuWellMaintained" relevant=" /location_details/sec3/canSeeRecipient =1 or  /location_details/sec3/canSeeRecipient =2" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec3/waterSupplier" required="true()" type="select1"/>
      <bind constraint="(regex(., '^[0-9]{1,4}$'))" jr:constraintMsg="O formato do valor deve ser XXXX (no max 9999mt)" nodeset="/location_details/sec3/waterMoney" required="true()" type="int"/>
      <bind nodeset="/location_details/hasSec4" type="select1"/>
      <bind nodeset="/location_details/sec4" relevant=" /location_details/hasSec4 =1"/>
      <bind nodeset="/location_details/sec4/hasLatrine" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec4/latrineType" relevant=" /location_details/sec4/hasLatrine =1" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec4/pooTreatment" relevant=" /location_details/sec4/hasLatrine =1" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec4/canSeeLatrine" relevant=" /location_details/sec4/hasLatrine =1" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec4/cleanedLatrine" relevant=" /location_details/sec4/canSeeLatrine =1 or  /location_details/sec4/canSeeLatrine =3" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec4/latrineHasPrivacy" relevant=" /location_details/sec4/canSeeLatrine =1 or  /location_details/sec4/canSeeLatrine =3" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec4/washAfterPoop" relevant=" /location_details/sec4/canSeeLatrine =1 or  /location_details/sec4/canSeeLatrine =3" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec4/otherPoopPlace" relevant=" /location_details/sec4/hasLatrine =2" required="true()" type="select1"/>
      <bind nodeset="/location_details/hasSec5" type="select1"/>
      <bind nodeset="/location_details/sec5" relevant=" /location_details/hasSec5 =1"/>
      <bind nodeset="/location_details/sec5/trashDeposit" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec5/trashDepositName" relevant=" /location_details/sec5/trashDeposit =98" required="true()" type="string"/>
      <bind nodeset="/location_details/sec5/trashTreatment" relevant=" /location_details/sec5/trashDeposit  != 3" required="true()" type="select1"/>
      <bind nodeset="/location_details/sec5/trashTreatmentName" relevant=" /location_details/sec5/trashTreatment =98" required="true()" type="string"/>
      <bind nodeset="/location_details/sec5/trashRecycledPart" relevant=" /location_details/sec5/trashTreatment =5" required="true()" type="string"/>
      <bind nodeset="/location_details/sec5/trashRecycleMethod" relevant=" /location_details/sec5/trashTreatment =5" required="true()" type="string"/>
      <bind nodeset="/location_details/sec5/trashUsedAsFertilizer" relevant=" /location_details/sec5/trashTreatment =7" required="true()" type="string"/>
      <bind nodeset="/location_details/ine" relevant="1=0"/>
      <bind nodeset="/location_details/ine/registered" required="true()" type="select1"/>
      <bind nodeset="/location_details/ine/reasonToNotReg" relevant=" /location_details/ine/registered =2" required="true()" type="select1"/>
      <bind nodeset="/location_details/ine/otherReasonToNotReg" relevant=" /location_details/ine/registered =2 and  /location_details/ine/reasonToNotReg =4" required="true()" type="string"/>
      <bind nodeset="/location_details/ine/tagAtDoor" relevant=" /location_details/ine/registered =1" required="true()" type="select1"/>
      <bind nodeset="/location_details/processedByMirth" readonly="true()" type="int"/>
      <bind nodeset="/location_details/processedBySync" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/location_details/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="concat(&quot;Info_Casa_&quot;, /location_details/houseNumber )" nodeset="/location_details/meta/instanceName" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/location_details/visitId">
      <label>1. Id da Visita</label>
    </input>
    <input ref="/location_details/roundNumber">
      <label>2. Número da ronda</label>
    </input>
    <input ref="/location_details/fieldWorkerId">
      <label>3. Código do Inquiridor</label>
    </input>
    <input ref="/location_details/locationId">
      <label>4. ExtId do Agregado</label>
    </input>
    <input ref="/location_details/houseNumber">
      <label>5. Número do Agregado</label>
    </input>
    <input ref="/location_details/headName">
      <label>7. Nome do Chefe de Agregado</label>
    </input>
    <input ref="/location_details/headPermId">
      <label>8. Perm Id do Chefe de Agregado</label>
    </input>
    <select1 ref="/location_details/hasSubsHead">
      <label>9. Esta possui chefe de Agregado Substituto?</label>
      <item>
        <label>Sim</label>
        <value>1</value>
      </item>
      <item>
        <label>Não</label>
        <value>2</value>
      </item>
    </select1>
    <input ref="/location_details/subsHeadName">
      <label>10. Nome do Chefe de Substituto</label>
    </input>
    <input ref="/location_details/subsHeadPermId">
      <label>11. Perm Id do Chefe de Substituto</label>
    </input>
    <group ref="/location_details/sec1">
      <label>A seguir perguntas da Secção 1: Caracteristícas e Condições da Habitação</label>
      <select1 ref="/location_details/sec1/houseNoLocated">
        <label>12. Localização do número do agregado?</label>
        <hint>Localização do número do agregado?</hint>
        <item>
          <label>Porta</label>
          <value>1</value>
        </item>
        <item>
          <label>Parede</label>
          <value>2</value>
        </item>
        <item>
          <label>Árvore</label>
          <value>3</value>
        </item>
        <item>
          <label>Porta de quintal</label>
          <value>4</value>
        </item>
        <item>
          <label>Outro Local</label>
          <value>98</value>
        </item>
      </select1>
      <select1 ref="/location_details/sec1/houseNoNeedRepaint">
        <label>13. A casa precisa ser repintada?</label>
        <hint>A casa precisa ser repintada?</hint>
        <item>
          <label>Sim</label>
          <value>1</value>
        </item>
        <item>
          <label>Não</label>
          <value>2</value>
        </item>
      </select1>
      <input ref="/location_details/sec1/nrConstructions">
        <label>14. Numero total de construções?</label>
        <hint>Nb: Incluir cozinha, celeiro, capoeira, garagem, casa de banho, barraca erguidas separadamente da casa</hint>
      </input>
      <input ref="/location_details/sec1/nrBedrooms">
        <label>14.1. Quantos destas construções são efetivamente casas (para dormir)?</label>
        <hint>Nb: excluir cozinha, celeiro, capoeira, garagem, casa de banho, barraca erguidas separadamente da casa</hint>
      </input>
      <input ref="/location_details/sec1/nrHouseDivisions">
        <label>15. Número de divisões usadas para dormir (mesmo que não sejam quartos)</label>
      </input>
      <input ref="/location_details/sec1/nrHouseDivUsed">
        <label>15.1. Quantas destas divisões foram usadas na ultima noite para dormir?</label>
      </input>
      <select ref="/location_details/sec1/habitationType">
        <label>16. Tipo de habitação:</label>
        <item>
          <label>Casa convencional</label>
          <value>1</value>
        </item>
        <item>
          <label>Flat/Apartamento</label>
          <value>2</value>
        </item>
        <item>
          <label>Palhota</label>
          <value>3</value>
        </item>
        <item>
          <label>Precário</label>
          <value>4</value>
        </item>
        <item>
          <label>Madeira/Zinco</label>
          <value>5</value>
        </item>
        <item>
          <label>Outro Especificar</label>
          <value>98</value>
        </item>
      </select>
      <input ref="/location_details/sec1/habitationName">
        <label>16.1. Especifique o tipo de habitação?</label>
      </input>
      <select1 ref="/location_details/sec1/wallMaterial">
        <label>17. Tipo de material de construção de paredes na casa principal:</label>
        <item>
          <label>Blocos de cimento</label>
          <value>1</value>
        </item>
        <item>
          <label>Bloco de tijolo</label>
          <value>2</value>
        </item>
        <item>
          <label>Madeira/zinco</label>
          <value>3</value>
        </item>
        <item>
          <label>Bloco de adobe</label>
          <value>4</value>
        </item>
        <item>
          <label>Caniço/pau/bambu/palmeira</label>
          <value>5</value>
        </item>
        <item>
          <label>Pau maticado</label>
          <value>6</value>
        </item>
        <item>
          <label>Lata/carvão/papel/saco/casca</label>
          <value>7</value>
        </item>
        <item>
          <label>Outro Especificar</label>
          <value>98</value>
        </item>
      </select1>
      <input ref="/location_details/sec1/wallMaterialName">
        <label>17.1. Especifique o tipo de material de construção das paredes?</label>
      </input>
      <select1 ref="/location_details/sec1/coverageMaterial">
        <label>18. Qual é o material usado para cobertura da casa principal?</label>
        <item>
          <label>Laje de Betão</label>
          <value>1</value>
        </item>
        <item>
          <label>Telha</label>
          <value>2</value>
        </item>
        <item>
          <label>Chapa de lusalite</label>
          <value>3</value>
        </item>
        <item>
          <label>Chapa de zinco</label>
          <value>4</value>
        </item>
        <item>
          <label>Capim/Palmeira</label>
          <value>5</value>
        </item>
        <item>
          <label>Outros</label>
          <value>98</value>
        </item>
      </select1>
      <input ref="/location_details/sec1/coverageMaterialName">
        <label>18.1. Especifique o tipo de material usado para cobertura da casa?</label>
      </input>
      <select1 ref="/location_details/sec1/floorMaterial">
        <label>19. Qual é o meterial usado no pavimento da casa principal?</label>
        <item>
          <label>Madeira/Parquet</label>
          <value>1</value>
        </item>
        <item>
          <label>Mármore/Granulito</label>
          <value>2</value>
        </item>
        <item>
          <label>Cimento</label>
          <value>3</value>
        </item>
        <item>
          <label>Mosaico/Tijoleira</label>
          <value>4</value>
        </item>
        <item>
          <label>Adobe/Terra batida</label>
          <value>5</value>
        </item>
        <item>
          <label>Sem nada</label>
          <value>6</value>
        </item>
        <item>
          <label>Outros (Especificar)</label>
          <value>98</value>
        </item>
      </select1>
      <input ref="/location_details/sec1/floorMaterialName">
        <label>19.1. Especifique o tipo de material usado no pavimento da casa?</label>
      </input>
      <input ref="/location_details/sec1/cellNumbers">
        <label>20. Pode nos fornecer o número de contactos telefónicos de alguns residentes da casa? (maximo 3)</label>
        <hint>Digite correctamente os numeros de telefone, caso não saiba coloque 88, se recusarem a responder coloque 99</hint>
      </input>
    </group>
    <group ref="/location_details/sec2">
      <label>A seguir perguntas da Secção 2: Caracteristícas da cozinha</label>
      <select1 ref="/location_details/sec2/hasKitchen">
        <label>21. Tem cozinha?</label>
        <item>
          <label>Sim</label>
          <value>1</value>
        </item>
        <item>
          <label>Não</label>
          <value>2</value>
        </item>
        <item>
          <label>Recusa</label>
          <value>3</value>
        </item>
      </select1>
      <select1 ref="/location_details/sec2/isKitchenInside">
        <label>22. A Cozinha está no interior da casa?</label>
        <item>
          <label>Sim</label>
          <value>1</value>
        </item>
        <item>
          <label>Não</label>
          <value>2</value>
        </item>
        <item>
          <label>Recusa</label>
          <value>3</value>
        </item>
      </select1>
      <select1 ref="/location_details/sec2/kitchenHasWall">
        <label>22.1. A cozinha tem paredes?</label>
        <item>
          <label>Sim</label>
          <value>1</value>
        </item>
        <item>
          <label>Não</label>
          <value>2</value>
        </item>
      </select1>
      <select1 ref="/location_details/sec2/kitchenHasCoverage">
        <label>22.2. A cozinha tem cobertura?</label>
        <item>
          <label>Sim</label>
          <value>1</value>
        </item>
        <item>
          <label>Não</label>
          <value>2</value>
        </item>
      </select1>
      <select1 ref="/location_details/sec2/kitchenFuel">
        <label>23. Que combustível usa para preparar os alimentos?</label>
        <item>
          <label>Lenha ou madeira</label>
          <value>1</value>
        </item>
        <item>
          <label>Carvão</label>
          <value>2</value>
        </item>
        <item>
          <label>Gás</label>
          <value>3</value>
        </item>
        <item>
          <label>Electricidade</label>
          <value>4</value>
        </item>
        <item>
          <label>Petróleo</label>
          <value>5</value>
        </item>
        <item>
          <label>Não usa nenhum combustível</label>
          <value>7</value>
        </item>
        <item>
          <label>Outros</label>
          <value>98</value>
        </item>
      </select1>
      <input ref="/location_details/sec2/kitchenFuelName">
        <label>23.1 Especifique a principal combustível usado para preparar os alimentos?</label>
      </input>
      <select1 ref="/location_details/sec2/iluminationFuel">
        <label>24. Principal fonte de energia usado para iluminação no agregado:</label>
        <item>
          <label>Electricidade</label>
          <value>1</value>
        </item>
        <item>
          <label>Gerador</label>
          <value>2</value>
        </item>
        <item>
          <label>Placa solar</label>
          <value>3</value>
        </item>
        <item>
          <label>Gás</label>
          <value>4</value>
        </item>
        <item>
          <label>Petróleo</label>
          <value>5</value>
        </item>
        <item>
          <label>Velas</label>
          <value>6</value>
        </item>
        <item>
          <label>Baterias</label>
          <value>7</value>
        </item>
        <item>
